@{
    ViewBag.Title = "租友推荐";
}
 
<link rel="stylesheet" type="text/css" href="/modules/css/rentBoyFr.css">
<div class="container"> 
    <div class="select-section">
        <div class="select">
            <p>我要租：</p>
            <select id="seachtype" >
                <option value="0">租男友</option>
                <option value="1">租女友</option>
            </select>
            <p>年龄：</p>
            <select id="seachage">
                <option value="">不限</option>
                <option value="16~25">16~25</option>
                <option value="26~32">26~32</option>
                <option value="33~38">33~38</option>
                <option value="39~44">39~44</option>
                <option value="45~51">45~51</option>
                <option value="52~60">52~60</option>
            </select>
            <p>地区：</p>
            <select class="province" name="province3"></select>&nbsp;<select class="city"  name="city3"></select>&nbsp;<select  class="district" name="area3"></select>
            <a href="/RFriend/RentFriend/1" id="seacha"></a>
            <div class="clear"></div>
        </div> 
    </div>
    <div class="rentGirlFr" style="min-height: 1100px;">
        <div class="left"> 
            <h3>租友推荐</h3>
            <ul id="recommentul"></ul>
            <div class="clear"></div>
            <div id="page"></div>
 
        </div>
        <div class="right">
            <div class="dynamic like">
                <h4>猜你喜欢<br/><small>Guess&nbsp;you&nbsp;like</small></h4>
                <ul id="likeul"></ul>
            </div>
            <div class="dynamic hire">
                <h4>最新需求<br/><small>latest&nbsp;demand</small></h4>
                <div class="myscroll3">
                    <ul id="needul"> 
                    </ul>
                </div>
            </div>
            <div class="ad"> </div>
        </div>
        <div class="clear"></div>
    </div>
    <div class="clear"></div>
    <input hidden="hidden" type="text" id="UserCity" style="display: none;" value="@ViewBag.UserAddress"/>
</div>
<script src="/modules/scripts/rfriend/recommend.js"></script>
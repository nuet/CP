<script type="text/javascript" src="/modules/scripts/index-script.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#sItem li:not(:first)").css("display", "none");
        var B = $("#sItem li:last");
        var C = $("#sItem li:first");
        setInterval(function () {
            if (B.is(":visible")) {
                C.fadeIn(500).addClass("in");
                B.hide();
            } else {
                $("#sItem li:visible").addClass("in");
                $("#sItem li.in").next().fadeIn(500);
                $("li.in").hide().removeClass("in")
            }
        }, 4000);  //每3秒钟切换一条，你可以根据需要更改  
        // 菜单点击事件
        $(".header .button2").on("click", function (evt) {
            $(".header .on2").removeClass("on2");
            $(this).addClass("on2");
        });
        $(".left-header .left-href a").on("click", function (evt) {
            $(".left-header .left-href .up").removeClass("up");
            $(this).addClass("up");
        }); 
      
    });
</script>

